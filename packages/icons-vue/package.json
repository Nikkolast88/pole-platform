    "camelcase": "^7.0.0",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "esbuild": "^0.14.49",
    "esbuild-plugin-globals": "^0.1.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^3.7.1",
    "unplugin-vue": "^2.2.1",
